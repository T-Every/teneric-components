{
  "name": "teneric-components",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "version": "0.1.4",
  "keywords": [
    "react",
    "components",
    "next"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T-Every/teneric-components.git"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "@company:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "test": "jest --runInBand",
    "setup": "pnpm i && cd lib-page && pnpm i && cd .. && pnpm run link",
    "link": "pnpm link ./lib-page/node_modules/react && pnpm link ./lib-page/node_modules/react-dom",
    "size": "size-limit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "bugs": {
    "url": "https://github.com/T-Every/teneric-components/issues"
  },
  "homepage": "https://github.com/T-Every/teneric-components#readme",
  "author": "t-every",
  "license": "ISC",
  "devDependencies": {
    "@mdx-js/react": "^2.3.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
