{
  "name": "teneric-components",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "version": "0.1.0",
  "keywords": [
    "react",
    "components",
    "next"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "headless",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T-Every/teneric-components.git"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "@company:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "jest --runInBand",
    "setup": "pnpm i && cd lib-page && pnpm i && cd .. && pnpm run link",
    "link": "pnpm link ./lib-page/node_modules/react && pnpm link ./lib-page/node_modules/react-dom",
    "size": "size-limit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/T-Every/teneric-components/issues"
  },
  "homepage": "https://github.com/T-Every/teneric-components#readme",
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "csstype": "^3.1.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.6",
    "tslib": "^2.4.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "goober": "^2.1.10"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "author": "t-every",
  "license": "ISC",
  "sideEffects": [
    "src/index.tsx"
  ]
}
