{
  "name": "teneric-components",
  "description": "A tevery library. Amazing and customizable components.",
  "version": "0.1.6",
  "keywords": [
    "react",
    "components",
    "next"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "headless",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T-Every/teneric-components.git"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "@company:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "jest --runInBand",
    "setup": "pnpm i && cd lib-page && pnpm i && cd .. && pnpm run link",
    "link": "pnpm link ./lib-page/node_modules/react && pnpm link ./lib-page/node_modules/react-dom",
    "size": "size-limit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "bugs": {
    "url": "https://github.com/T-Every/teneric-components/issues"
  },
  "homepage": "https://github.com/T-Every/teneric-components#readme",
  "author": "tevery",
  "license": "ISC",
  "dependencies": {
    "@swc/core": "^1.3.76",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "csstype": "^3.1.2",
    "goober": "^2.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "sideEffects": [
    "src/index.tsx"
  ]
}
